              </div>
              <div class="ms-3">
                <p class="mb-0">Personal : <%= retry_amount %> $</p>
                <p class="mt-2 mb-0">Business : <%= retry_amount %> $</p>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- end col-->

      <div class="col-sm-6">
        <div class="card widget-flat h-100">
          <div class="card-header">
            <h4 class="card-title text-center">IDF Status</h4>
          </div>
          <div class="card-body">
            <div class="d-flex align-items-center">
              <div class="me-auto text-center">
                <a href="/payments/all_gateway_dt">
                  <button class="btn-secondary">
                    <span class="text-success me-2"><i class="mdi mdi-arrow-up-bold"></i>
                      <%= consolidated_count %> files
                    </span>
                  </button>
                </a>
              </div>
              <div class="ms-3">
                <p class="mb-0">Perfected : <%= consolidated_debit_total %> $</p>
                <p class="mt-2 mb-0">Expired : <%= consolidated_credit_total %> $</p>
              </div>
            </div>
          </div>
        </div>
      </div> <!-- end col-->
    </div> <!-- end row -->

    <div class="my-2"></div> <!-- Ajoute un espace de 2 unités entre les deux divs -->
  </div> <!-- end col -->

  <div class="col-xl-6 col-lg-6">
    <div class="row">
      <div class="col-sm-6">
        <div class="card widget-flat h-100">
          <div class="card-header">
            <h4 class="card-title text-center">Case management</h4>
          </div>
          <div class="card-body">
            <div class="d-flex align-items-center">
              <div class="me-auto text-center">
                <a href="/retry/zenith_bk_gh_dt">
                  <button class="btn-secondary">
                    <span class="text-success me-2"><i class="mdi mdi-arrow-up-bold"></i>
                      <%= retry_count %> files
                    </span>
